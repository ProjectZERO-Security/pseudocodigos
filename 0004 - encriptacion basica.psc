Proceso encriptacion_basica
	
	//	NOMBRE			:	Esteban Aros C.
	//	N° EJERCICIO	:	0004
	//	PREGUNTA		:	Crear un algoritmo de encriptacion basico
	
	Definir letras Como Caracter;
	
	letraencriptada <- 'A';
	
	Escribir "Ingrese letra";
	Leer letras;
	
	letraencriptada <- letras;
	
	Escribir "Resultado de la encriptación: ",letraencriptada;
	
	
FinProceso
